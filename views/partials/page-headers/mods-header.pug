div.panel
  div.panel-heading
    div.panel-heading-header
      h3 My Mods  
    div.panel-heading-content
      ul#mod-list
        each mod in mods
          li
            div.mod-info
              h3.mod-title #[a(href=`//${nconf.domain}/mods/${mod.id}`)=`${mod.name}`]
              p.mod-hidden=`${mod.description}`
  div.panel-body#mods-background
